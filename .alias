### aliases ###
alias ll='ls -FGlAhp'
alias docker-env='eval "$(docker-machine env default)"'
alias cdwork='cd ~/Documents/workspace'
alias cdhris='cd ~/Documents/workspace/hris'
alias cdcore='cd /Volumes/workspace/work/core'
alias tmux='TERM=screen-256color tmux'
alias dockerstopall='docker stop $(docker ps -a -q)'
alias fix_stty='stty sane'
alias ffind='find * -type f | fzf'
alias ghc-mod-install='stack install hlint ghc-mod hdevtools'
alias brew-update='brew update && brew upgrade && brew cleanup'
alias ctrlp-root='touch .ctrlp'
alias hdevtools-stop='pkill hdevtools'
